import java.util.*;
public class emi
{
    public static void main(String[] args)
    {
        int p, r, n=5, i;
        System.out.print(" Enter Principle Amount : ");
        Scanner s1 = new Scanner(System.in);
        p = s1.nextInt();
        System.out.print(" Enter Rate Of Interest : ");
        s1 = new Scanner(System.in);
        
        r = s1.nextInt();
        i = (p * r * n) / 100;
        double emi = i / 60;
        System.out.println("EMI is " + emi);
    }
}
